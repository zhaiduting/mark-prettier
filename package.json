{
	"name": "mark-prettier",
	"version": "1.0.0",
	"description": "Format Markdown files in Obsidian using Prettier.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian-plugin",
		"prettier",
		"markdown",
		"format"
	],
	"author": "zhaiduting",
	"authorEmail": "zhaiduting@163.com",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"prettier-plugin-toml": "^2.0.6",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
